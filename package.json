{
  "name": "permobil-client",
  "version": "0.1.0",
  "scripts": {
    "*************************** GITHUB ACTIONS CI SPECIFIC SCRIPTS": "***************************",
    "ci.build.base.setup": "echo no | npm i -g nativescript && ns usage-reporting disable && ns error-reporting disable && npm i && npm run global.installs",
    "ci.pip.install": "sudo pip install --upgrade pip && sudo pip install six",
    "ci.build.pt.mobile.setup": "npm run ci.build.base.setup && npm run setup.@permobil && npm run setup.mobile",
    "ci.pt.m.android": "cd apps/mobile/pushtracker && ns build android --env.aot --env.uglify",
    "ci.pt.m.ios": "cd apps/mobile/pushtracker && ns build ios --env.aot --env.uglify",
    "ci.build.pt.wear.setup": "npm run ci.build.base.setup && npm run setup.@permobil && npm run setup.pt.wear",
    "ci.build.sd.wear.setup": "npm run ci.build.base.setup && npm run setup.@permobil && npm run setup.sd.wear",
    "ci.pt.w.android": "cd apps/wear/pushtracker && ns build android --env.uglify",
    "ci.sd.w.android": "cd apps/wear/smartdrive && ns build android --env.uglify",
    "*************************** /end GITHUB ACTIONS CI SPECIFIC SCRIPTS": "***************************",
    "tslint": "tslint \"**/*.ts\" --config tslint.json --exclude \"**/node_modules/**\"  --exclude \"**/platforms/**\"  --exclude \"**/hooks/**\" --exclude \"**/typings/**\" --exclude \"**/*d.ts\"",
    "global.installs": "npm i -g rimraf && npm i -g typescript@3.9.7",
    "pretty-baby": "prettier --write '@(apps|@permobil|forked-plugins)/**/*.@(scss|component.html|xml|ts|md|js|json)'",
    "*************************** SETUP": "***************************",
    "setup": "npm run global.installs && npm run setup.@permobil && npm run setup.mobile && npm run setup.wear",
    "setup.all": "npm i && npm run global.installs && npm run setup.all.non-global",
    "setup.all.non-global": "npm i && npm run setup.@permobil && npm run setup.mobile && npm run setup.wear",
    "setup.mobile": "npm run setup.pt.mobile",
    "setup.se.mobile": "cd apps/mobile/smart-eval && npm i",
    "setup.pt.mobile": "cd apps/mobile/pushtracker && npm i",
    "setup.wear": "npm run setup.sd.wear && npm run setup.pt.wear",
    "setup.sd.wear": "cd apps/wear/smartdrive && npm i && npm run scss",
    "setup.pt.wear": "cd apps/wear/pushtracker && npm i && npm run scss",
    "setup.@permobil": "npm run setup.@permobil/angular && npm run setup.@permobil/core && npm run setup.@permobil/nativescript && npm run setup.@permobil/wear-os && cd @permobil && tsc",
    "setup.@permobil/angular": "cd @permobil/angular && npm i",
    "setup.@permobil/core": "cd @permobil/core && npm i",
    "setup.@permobil/nativescript": "cd @permobil/nativescript && npm i",
    "setup.@permobil/wear-os": "cd @permobil/nativescript/src/plugins/nativescript-wear-os-comms/src && npm i",
    "*************************** end SETUP": "***************************",
    "*************************** CLEAN SECTION": "***************************",
    "clean": "rimraf hooks node_modules package-lock.json",
    "clean.all": "npm run clean && npm run clean.@permobil && npm run clean.mobile && npm run clean.wear",
    "clean.mobile": "npm run clean.pt.mobile && npm run clean se.mobile",
    "clean.pt.mobile": "cd apps/mobile/pushtracker && ns clean",
    "clean.se.mobile": "cd apps/mobile/smart-eval && ns clean",
    "clean.wear": "npm run clean.pt.wear && npm run clean.sd.wear",
    "clean.pt.wear": "cd apps/wear/pushtracker && ns clean",
    "clean.sd.wear": "cd apps/wear/smartdrive && ns clean",
    "clean.@permobil": "npm run clean.@permobil.angular && npm run clean.@permobil.nativescript",
    "clean.@permobil.angular": "cd @permobil/angular && rimraf hooks node_modules package-lock.json",
    "clean.@permobil.nativescript": "cd @permobil/nativescript && rimraf hooks node_modules package-lock.json",
    "nuki": "npm run clean.all && npm run setup.all",
    "*************************** end CLEAN SECTION": "***************************",
    "*************************** PushTracker Mobile ***************************": "***************************",
    "pt.m.apk.from.aab": "cd ./tools && java -jar bundletool-all-0.12.0.jar build-apks --bundle=../apps/mobile/pushtracker/tools/pushtracker.aab --output=ptm.apks --mode=universal",
    "pt.m.clean": "cd apps/mobile/pushtracker && ns clean",
    "pt.m.a": "cd apps/mobile/pushtracker && ns debug android --env.aot",
    "pt.m.a.nohmr": "cd apps/mobile/pushtracker && ns debug android --env.aot --no-hmr",
    "pt.m.start.android": "cd apps/mobile/pushtracker && ns debug android --env.aot",
    "pt.m.start.android.report": "cd apps/mobile/pushtracker && ns run android --env.aot --env.report",
    "pt.m.i": "cd apps/mobile/pushtracker && ns debug ios --env.aot",
    "pt.m.i.nohmr": "cd apps/mobile/pushtracker && ns debug ios --env.aot --no-hmr",
    "pt.m.start.ios": "cd apps/mobile/pushtracker && ns debug ios --env.aot",
    "pt.m.start.ios.report": "cd apps/mobile/pushtracker && ns run ios --env.aot --env.report",
    "pt.m.ios.release": "cd apps/mobile/pushtracker && ns build ios --release --no-hmr --env.aot --env.uglify",
    "pt.m.android.release": "cd apps/mobile/pushtracker/tools && node ./android-release.js",
    "pt.m.android.layouts": "adb shell dumpsys gfxinfo com.permobil.pushtracker > layout-profile.txt",
    "pt.m.android.framestats": "adb shell dumpsys gfxinfo com.permobil.pushtracker framestats > framestats.txt",
    "*************************** end PushTracker Mobile ***************************": "***************************",
    "*************************** Smart Eval ***************************": "***************************",
    "se.apk.from.aab": "cd ./tools && java -jar bundletool-all-0.12.0.jar build-apks --bundle=../apps/mobile/smart-eval/tools/smart-eval.aab --output=se.apks --mode=universal",
    "se.clean": "cd apps/mobile/smart-eval && ns clean",
    "se.a": "cd apps/mobile/smart-eval && ns debug android --env.aot",
    "se.start.android": "cd apps/mobile/smart-eval && ns debug android --env.aot",
    "se.start.android.report": "cd apps/mobile/smart-eval && ns debug android --env.aot --env.report",
    "se.i": "cd apps/mobile/smart-eval && ns debug ios --env.aot",
    "se.start.ios": "cd apps/mobile/smart-eval && ns debug ios --env.aot",
    "se.start.ios.report": "cd apps/mobile/smart-eval && ns run ios --env.aot --env.report",
    "se.ios.release": "npm run nuki && cd apps/mobile/smart-eval && ns build ios --release --no-hmr --env.aot --env.uglify",
    "se.android.release": "cd apps/mobile/smart-eval/tools && node ./android-release.js",
    "se.android.layouts": "adb shell dumpsys gfxinfo com.permobil.smartevaluation > layout-profile.txt",
    "se.android.framestats": "adb shell dumpsys gfxinfo com.permobil.smartevaluation framestats > framestats.txt",
    "*************************** end Smart Eval": "***************************",
    "//*************************** SmartDrive Wear": "***************************",
    "sd.w.apk.from.aab": "cd ./tools && java -jar bundletool-all-0.12.0.jar build-apks --bundle=../apps/wear/smartdrive/tools/smartdrive-wearos.aab --output=sdw.apks --mode=universal",
    "sd.w.clean": "npm run clean.sd.wear",
    "sd.w.setup": "npm run setup.sd.wear",
    "sd.w.brad": "cd apps/wear/smartdrive && ns debug android --device 8606X91120272",
    "sd.w.a": "cd apps/wear/smartdrive && ns debug android",
    "sd.w.a.nohmr": "cd apps/wear/smartdrive && ns debug android --no-hmr",
    "sd.w.a.report": "cd apps/wear/smartdrive && ns run android  --env.report",
    "sd.w.android.release": "cd apps/wear/smartdrive/tools && node ./android-release.js",
    "sd.w.android.layouts": "adb shell dumpsys gfxinfo com.permobil.smartdrive.wearos > layout-profile.txt",
    "sd.w.android.framestats": "adb shell dumpsys gfxinfo com.permobil.smartdrive.wearos framestats > framestats.txt",
    "//************************** end SmartDrive Wear": "***************************",
    "//************************** PushTracker Wear": "***************************",
    "pt.w.apk.from.aab": "cd ./tools && java -jar bundletool-all-0.12.0.jar build-apks --bundle=../apps/wear/pushtracker/tools/pushtracker-wear.aab --output=ptw.apks --mode=universal",
    "pt.w.clean": "npm run clean.pt.w",
    "pt.w.setup": "npm run setup.pt.w",
    "pt.w.brad": "cd apps/wear/pushtracker && ns debug android --device 8606X91120272",
    "pt.w.a": "cd apps/wear/pushtracker && ns debug android",
    "pt.w.a.nohmr": "cd apps/wear/pushtracker && ns debug android --no-hmr",
    "pt.w.a.report": "cd apps/wear/pushtracker && ns run android --env.report",
    "pt.w.android.release": "cd apps/wear/pushtracker/tools && node ./android-release.js",
    "pt.w.android.layouts": "adb shell dumpsys gfxinfo com.permobil.pushtracker.wearos > layout-profile.txt",
    "pt.w.android.framestats": "adb shell dumpsys gfxinfo com.permobil.pushtracker.wearos framestats > framestats.txt",
    "//************************** end PushTracker Wear": "***************************"
  },
  "dependencies": {
    "@nativescript/core": "~7.0.10",
    "@nativescript/angular": "~10.1.5",
    "@nstudio/nativescript-loading-indicator": "~4.0.0",
    "@nstudio/nativescript-pulltorefresh": "~3.0.1",
    "@nstudio/nativescript-snackbar": "~2.0.2",
    "email-validator": "~2.0.4"
  },
  "devDependencies": {
    "@nativescript/types": "~7.0.4",
    "@nativescript/webpack": "~3.0.7",
    "@types/lodash": "~4.14.161",
    "husky": "~4.3.0",
    "lint-staged": "~10.4.0",
    "mkdirp": "~1.0.4",
    "node-fetch": "~2.6.1",
    "node-sass": "~4.14.1",
    "prettier": "~2.1.2",
    "prompts": "~2.3.2",
    "rimraf": "~3.0.2",
    "semver": "~7.3.2",
    "tslint": "~6.1.3",
    "typescript": "~3.9.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run tslint"
    }
  },
  "lint-staged": {
    "**/*.{js, css, ts, json, scss, html, xml, md}": [
      "prettier --write",
      "git add"
    ]
  }
}
