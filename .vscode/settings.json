{
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/typings/**": true,
    "**/hooks/**": true,
    "**/platforms/**": true,
    "**/node_modules/**": true,
    "apps/mobile/smart-eval-app/node_modules/**": true,
    "forked-plugins/nativescript-bluetooth/node_modules/**": true,
    "nativescript-bluetooth/typings/**": true
  },
  "[html]": {
    "editor.formatOnSave": false
  },
  "scss.lint.validProperties": [
    "horizontal-align",
    "android-elevation",
    "android-dynamic-elevation-offset",
    "translate-y",
    "translate-x",
    "text-wrap",
    "off-background-color"
  ],
  "css.lint.validProperties": [
    "horizontal-align",
    "android-elevation",
    "android-dynamic-elevation-offset",
    "translate-y",
    "translate-x",
    "text-wrap",
    "off-background-color"
  ],
  "prettier.disableLanguages": []
}
